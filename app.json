{
  "expo": {
    "name": "onHand",
    "slug": "onhand",
    "scheme": "onhand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_onhand-fleur.png",
    "splash": {
      "image": "./assets/logo_onhand-fleur.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onhand.onHand"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.onhand",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "8ad9526a-233e-40f3-b3b3-0ef0b1d3ab2c"
      }
    }
  }
}
